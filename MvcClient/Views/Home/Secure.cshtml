@using Microsoft.AspNetCore.Authentication
@model List<string>

<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>

<dl>
    @foreach (var value in Model)
    {
        <dt>Value</dt>
        <dd>@value</dd>
    }
</dl>